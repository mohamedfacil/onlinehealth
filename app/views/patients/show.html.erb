<h1 class = "text-dark text-center bg-light" ><%= @patient.patient_name %> </h1>

<nav class="navbar navbar-dark bg-dark justify-content-between"  >
  <a class="navbar-brand">Navbar</a>
  <div class="row">
    <input class="form-control " type="search" placeholder="Search" aria-label="Search">
    <button class="btn btn-outline-success my-2 btn-sm" type="submit">Search</button>
  </div>
</nav>
<br>
<h1 class = "text-dark text-center bg-light">Doctor List </h1>

 <table class="table table-striped table-hover table-bordered table-sm">
  <thead class="table-dark">
    <tr>
      <th>Doctor name</th>
      <th>Address</th>
      <th>Department</th>
      <th>Qualification</th>
      <th>Dob</th>
      <th>Gender</th>
      <th>Email</th>
      <th>Phone</th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.doctor_name %></td>
        <td><%= doctor.address %></td>
        <td><%= doctor.department %></td>
        <td><%= doctor.qualification %></td>
        <td><%= doctor.dob %></td>
        <td><%= doctor.gender %></td>
        <td><%= doctor.email %></td>
        <td><%= doctor.phone %></td>
      </tr>
    <% end %>
  </tbody>
</table>


